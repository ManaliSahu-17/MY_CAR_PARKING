<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#63DCEC"
    tools:context=".Menu"
    android:fitsSystemWindows="true">

    <TextView
        android:id="@+id/text2"
        android:layout_width="match_parent"
        android:layout_height="155dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="0dp"
        android:background="#FFEB3B" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="111dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-7dp"
        android:layout_marginTop="52dp"
        app:srcCompat="@drawable/img1" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="154dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="9dp"
        android:layout_marginTop="130dp"
        android:hint="HELLO,USER"
        android:textColorHint="#0B0A0A"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/text4"
        android:layout_width="295dp"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="114dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="2dp"
        android:hint="CAR PARKING"
        android:textAlignment="center"
        android:textColorHint="#0E0E0E"
        android:textSize="40sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="198dp"
        android:hint="Parking is the act of stopping and disengaging a vehicle"
        android:textAlignment="center"
        android:textColorHint="#090909"
        android:textSize="16sp"
        android:textStyle="bold" />


    <Button
        android:id="@+id/button8"
        android:layout_width="146dp"
        android:layout_height="113dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="253dp"
        android:background="@drawable/googlemap" />

    <Button
            android:id="@+id/button9"
            android:layout_width="140dp"
            android:layout_height="109dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="254dp"
            android:layout_marginEnd="28dp"
            android:background="@drawable/locationarea" />

        <Button
            android:id="@+id/button10"
            android:layout_width="137dp"
            android:layout_height="99dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="15dp"
            android:layout_marginBottom="210dp"
            android:background="@drawable/parkingstatus" />

        <Button
        android:id="@+id/button11"
        android:layout_width="138dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="193dp"
        android:background="@drawable/parkingtracker" />

    <Button
        android:id="@+id/button12"
        android:layout_width="148dp"
        android:layout_height="98dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="39dp"
        android:background="@drawable/calendar" />

    <Button
        android:id="@+id/button13"
        android:layout_width="149dp"
        android:layout_height="91dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="41dp"
        android:background="@drawable/feedback" />



</RelativeLayout>